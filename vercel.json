{
    "version": 2,
    "builds": [
        {
            "src": "/api/index.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "public/**"
                ]
            }
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ]
}