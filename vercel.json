{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
      ]
    }
  ],
  "builds": [
    {
      "src": "/api/index.ts",
      "use": "@vercel/node"
    }
  ]
}